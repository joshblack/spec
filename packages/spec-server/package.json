{
  "name": "@spec/server",
  "version": "1.1.2",
  "license": "MIT",
  "author": "Josh Black <joshblack@us.ibm.com>",
  "files": [
    "middleware",
    "tools",
    "types.js",
    "ssl"
  ],
  "repository": "https://github.com/joshblack/spec/tree/master/packages/spec-server",
  "bugs": {
    "url": "https://github.com/joshblack/spec/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn types:remove && yarn types:publish",
    "clean": "rimraf middleware tools types.js types.js.flow",
    "prepublish": "yarn build",
    "types:publish": "flow-copy-source -i \"**/{__tests__,__mocks__}/**/*.js\" src .",
    "types:remove": "flow-remove-types -q -i \"(__tests__|__mocks__)\" --out-dir . src",
    "watch": "watch \"yarn build\" src"
  },
  "dependencies": {
    "@spec/config": "1.1.2",
    "body-parser": "1.17.2",
    "compression": "1.7.0",
    "config": "1.26.2",
    "del": "^3.0.0",
    "express": "4.15.4",
    "helmet": "3.8.1",
    "hpp": "0.2.2",
    "morgan": "1.8.2",
    "selfsigned": "1.10.1",
    "strict-transport-security": "^0.2.0",
    "uuid": "^3.1.0",
    "webpack": "3.5.6",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.19.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "flow-copy-source": "1.2.1",
    "flow-remove-types": "1.2.1",
    "freeport": "1.0.5",
    "rimraf": "2.6.1",
    "watch": "1.0.2"
  }
}

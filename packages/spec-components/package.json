{
  "name": "@spec/components",
  "version": "0.1.0",
  "description": "Set of helper components for developing React-based apps.",
  "scripts": {
    "build": "yarn clean && yarn types:remove && yarn types:publish",
    "clean": "rimraf lib",
    "prepublish": "yarn build",
    "types:publish": "flow-copy-source -i \"**/{__tests__,__mocks__}/**/*.js\" src lib",
    "types:remove": "flow-remove-types -q -i \"__tests__\" -i \"__mocks__\" --out-dir lib src"
  },
  "author": "Josh Black <joshblack@us.ibm.com>",
  "license": "MIT",
  "dependencies": {
    "prop-types": "15.5.10"
  },
  "devDependencies": {
    "flow-copy-source": "1.2.1",
    "flow-remove-types": "1.2.1",
    "react": "15.6.1",
    "react-intl": "2.4.0",
    "react-test-renderer": "15.6.1",
    "rimraf": "2.6.2"
  },
  "peerDependencies": {
    "react": "15.6.1",
    "react-intl": "2.4.0"
  }
}
